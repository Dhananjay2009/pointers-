#include<stdio.h>

int main()
{
    int a=76;
    int *ptra = &a;
    int *ptr2 = NULL;
    printf("The value of a is %d\n", *ptra);
    printf("The value of a is %x\n", &ptra);
    printf("The value of a is %p\n", &ptra);
    printf("The value of a is %p\n", &a);
    printf("The address of some garbage is %d\n", *ptr2);
    return 0;
}
